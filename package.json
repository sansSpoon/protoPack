{
	"name": "protoPack",
	"version": "0.1.0",
	"description": "A basic boiler for Webpack enabled projects - vanilla front-end",
	"main": "index.js",
	"private": true,
	"scripts": {
		"dev": "webpack --config webpack.config.dev.js --mode development",
		"prd": "webpack --config webpack.config.prd.js --mode production",
		"start": "webpack-dev-server --config webpack.config.dev.js --open"
	},
	"keywords": [
		"webpack",
		"sass",
		"stylelint",
		"babel",
		"eslint",
		"boilerplate"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sansSpoon/protoPack.git"
	},
	"homepage": "https://github.com/sansspoon/protoPack#readme",
	"bugs": {
		"url": "https://github.com/sansspoon/protoPack/issues"
	},
	"author": "sansSpoon",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^8.6.5",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
		"babel-preset-airbnb": "^2.5.1",
		"babel-preset-env": "^1.7.0",
		"css-loader": "^1.0.0",
		"cssnano": "^4.0.1",
		"eslint": "^4.19.1",
		"eslint-config-airbnb-base": "^13.0.0",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-import": "^2.13.0",
		"mini-css-extract-plugin": "^0.4.1",
		"node-sass": "^4.9.2",
		"postcss-loader": "^2.1.6",
		"sass-loader": "^7.0.3",
		"style-loader": "^0.21.0",
		"stylelint": "^9.3.0",
		"stylelint-config-standard": "^18.2.0",
		"stylelint-order": "^0.8.1",
		"stylelint-webpack-plugin": "^0.10.5",
		"url-loader": "^1.0.1",
		"webpack": "^4.16.0",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.4"
	}
}
